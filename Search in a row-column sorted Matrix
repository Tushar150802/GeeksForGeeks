class Solution 
{ 
	static boolean search(int matrix[][], int n, int m, int x) 
	{  
	    int i=0;
	    int j=n-1;
	    while(i<n && j>0)
	    {
	        if (matrix[i][j]==x)
	        return true;
	        else if(matrix[i][j]<x)
	        i++;
	        else j--;
	    }
	    return false;
	} 
}

O(n)
O(1)
